<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:background="@mipmap/icon_bg"
        android:orientation="vertical"
        android:layout_height="match_parent">
    <include layout="@layout/titlebusiness"></include>
    <LinearLayout
            android:layout_width="wrap_content"
            android:layout_gravity="center_horizontal"
            android:background="@mipmap/icon_jingweidu"
            android:layout_marginTop="@dimen/y70"
            android:layout_height="wrap_content">

        <RelativeLayout
                android:layout_width="@dimen/y718"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_height="@dimen/y335">
            <LinearLayout
                    android:layout_width="wrap_content"
                    android:gravity="center"
                    android:id="@+id/llContent"
                    android:orientation="horizontal"
                    android:layout_height="wrap_content">
                <TextView
                        android:layout_width="@dimen/y160"
                        android:layout_height="wrap_content"
                        android:text="经度"
                        android:alpha="0.75"
                        android:textSize="@dimen/y36"
                        android:textColor="@color/color_fffffff"/>
                <LinearLayout
                        android:layout_width="wrap_content"
                        android:orientation="vertical"
                        android:layout_height="@dimen/y81">
                    <EditText
                            android:layout_width="@dimen/y340"
                            android:textSize="@dimen/y60"
                            android:textColor="@color/color_fffffff"
                            android:text="50"
                            android:gravity="center"
                            android:background="@null"
                            android:layout_height="@dimen/y80"/>
                    <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/y1"
                            android:background="@color/color_fffffff">
                    </View>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                    android:layout_width="wrap_content"
                    android:gravity="center"
                    android:layout_below="@+id/llContent"
                    android:orientation="horizontal"
                    android:layout_height="wrap_content">
                <TextView
                        android:layout_width="@dimen/y160"
                        android:layout_height="wrap_content"
                        android:text="纬度："
                        android:alpha="0.75"
                        android:textSize="@dimen/y36"
                        android:textColor="@color/color_fffffff"/>
                <LinearLayout
                        android:layout_width="wrap_content"
                        android:orientation="vertical"
                        android:layout_height="@dimen/y81">
                    <EditText
                            android:layout_width="@dimen/y340"
                            android:textSize="@dimen/y60"
                            android:textColor="@color/color_fffffff"
                            android:text="100"
                            android:gravity="center"
                            android:background="@null"
                            android:layout_height="@dimen/y80"/>
                    <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/y1"
                            android:background="@color/color_fffffff">
                    </View>
                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>

    </LinearLayout>
    <LinearLayout
            android:layout_width="wrap_content"
            android:layout_gravity="center_horizontal"
            android:background="@mipmap/icon_jingweidu"
            android:layout_marginTop="@dimen/y20"
            android:layout_height="wrap_content">

        <RelativeLayout
                android:layout_width="@dimen/y718"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_height="@dimen/y335">
            <LinearLayout
                    android:layout_width="wrap_content"
                    android:gravity="center"
                    android:id="@+id/llContent2"
                    android:orientation="horizontal"
                    android:layout_height="wrap_content">
                <TextView
                        android:layout_width="@dimen/y160"
                        android:layout_height="wrap_content"
                        android:text="经度"
                        android:alpha="0.75"
                        android:textSize="@dimen/y36"
                        android:textColor="@color/color_fffffff"/>
                <LinearLayout
                        android:layout_width="wrap_content"
                        android:orientation="vertical"
                        android:layout_height="@dimen/y81">
                    <EditText
                            android:layout_width="@dimen/y340"
                            android:textSize="@dimen/y60"
                            android:textColor="@color/color_fffffff"
                            android:text="50"
                            android:gravity="center"
                            android:background="@null"
                            android:layout_height="@dimen/y80"/>
                    <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/y1"
                            android:background="@color/color_fffffff">
                    </View>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                    android:layout_width="wrap_content"
                    android:gravity="center"
                    android:layout_below="@+id/llContent2"
                    android:orientation="horizontal"
                    android:layout_height="wrap_content">
                <TextView
                        android:layout_width="@dimen/y160"
                        android:layout_height="wrap_content"
                        android:text="纬度："
                        android:alpha="0.75"
                        android:textSize="@dimen/y36"
                        android:textColor="@color/color_fffffff"/>
                <LinearLayout
                        android:layout_width="wrap_content"
                        android:orientation="vertical"
                        android:layout_height="@dimen/y81">
                    <EditText
                            android:layout_width="@dimen/y340"
                            android:textSize="@dimen/y60"
                            android:textColor="@color/color_fffffff"
                            android:text="100"
                            android:gravity="center"
                            android:background="@null"
                            android:layout_height="@dimen/y80"/>
                    <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/y1"
                            android:background="@color/color_fffffff">
                    </View>
                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
            android:layout_width="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/y60"
            android:layout_height="wrap_content">
        <Button
                android:layout_width="@dimen/y340"
                android:layout_height="@dimen/y74"
                android:background="@mipmap/icon_button"
                android:textSize="@dimen/y36"

                android:text="保存"/>
        <Button android:layout_width="@dimen/y340"
                android:layout_height="@dimen/y74"
                android:background="@mipmap/icon_button2"
                android:layout_marginLeft="@dimen/y40"
                android:textColor="@color/color_fffffff"
                android:textSize="@dimen/y36"
                android:text="添加"/>
    </LinearLayout>
</LinearLayout>